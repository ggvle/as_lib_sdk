<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.yline.view.demo">

    <application
        android:name="com.yline.application.BaseApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".SimpleListActivity"/>
        <activity android:name=".SimpleRecyclerActivity"/>
        <activity android:name=".SimpleGridDecorationActivity">
        </activity>
        <activity android:name=".SimpleLinearDecorationActivity">
        </activity>
        <activity android:name=".HeadFootRecyclerActivity">
        </activity>
    </application>

</manifest>